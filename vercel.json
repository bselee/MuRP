{
  "buildCommand": "npm run build",
  "functions": {
    "api/**/*.ts": {
      "includeFiles": "lib/**/*"
    }
  },
  "crons": [
    {
      "path": "/api/trigger-finale-sync",
      "schedule": "0 2,8,14,20 * * *"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
